        <relativePath>../../web-plugin-base</relativePath>
    </parent>

    <artifactId>visallo-web-zip-code-boundaries</artifactId>
    <name>Visallo: Web: Plugin: Zip Code Boundaries</name>

    <properties>
        <geotools.version>12-RC1</geotools.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.geotools</groupId>
            <artifactId>gt-shapefile</artifactId>
            <version>${geotools.version}</version>
