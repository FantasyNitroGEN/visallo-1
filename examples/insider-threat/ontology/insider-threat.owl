        <visallo:glyphIconFileName>401kTransaction.png</visallo:glyphIconFileName>
        <visallo:titleFormula xml:lang="en">
            var title = &apos;401k &apos;;
            var amount = parseInt(propRaw(&apos;http://visallo.org/insider-threat#transactionAmount&apos;));
