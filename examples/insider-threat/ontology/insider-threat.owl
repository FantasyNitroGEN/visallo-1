    <!-- http://visallo.org/insider-threat#address -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/insider-threat#address">
        <rdfs:label xml:lang="en">Address</rdfs:label>
        <visallo:displayFormula>
            _.compact([
            dependentProp(&apos;http://visallo.org/insider-threat#street1&apos;),
            dependentProp(&apos;http://visallo.org/insider-threat#street2&apos;),
            dependentProp(&apos;http://visallo.org/insider-threat#city&apos;),
            dependentProp(&apos;http://visallo.org/insider-threat#state&apos;),
            dependentProp(&apos;http://visallo.org/insider-threat#zipCode&apos;)
            ]).join(&apos;, &apos;)
        </visallo:displayFormula>
        <visallo:dependentPropertyIris>[
            &quot;http://visallo.org/insider-threat#street1&quot;,
            &quot;http://visallo.org/insider-threat#street2&quot;,
            &quot;http://visallo.org/insider-threat#city&quot;,
            &quot;http://visallo.org/insider-threat#state&quot;,
            &quot;http://visallo.org/insider-threat#zipCode&quot;,
            &quot;http://visallo.org/insider-threat#geoLocation&quot;
            ]</visallo:dependentPropertyIris>
        <rdfs:domain rdf:resource="http://visallo.org/insider-threat#employee"/>
        <rdfs:domain rdf:resource="http://visallo.org/insider-threat#facility"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


