    <!-- http://visallo.org/insider-threat#applicationRiskMessage -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/insider-threat#applicationRiskMessage">
        <rdfs:label xml:lang="en">Application Risk</rdfs:label>
        <visallo:addable rdf:datatype="&xsd;boolean">false</visallo:addable>
        <visallo:displayFormula>
            var risk = prop(&apos;http://visallo.org/insider-threat#applicationRisk&apos;);
            var riskReason = prop(&apos;http://visallo.org/insider-threat#applicationRiskName&apos;);
            if (riskReason) {
                riskReason = &apos;(&apos; + riskReason + &apos;)&apos;;
            }
            return _.compact([risk, riskReason]).join(&apos; &apos;);
        </visallo:displayFormula>
        <visallo:dependentPropertyIris>[&apos;http://visallo.org/insider-threat#applicationRisk&apos;,&apos;http://visallo.org/insider-threat#applicationRiskName&apos;]</visallo:dependentPropertyIris>
        <visallo:propertyGroup xml:lang="en">Access Risk</visallo:propertyGroup>
        <visallo:displayType xml:lang="en">risk</visallo:displayType>
        <rdfs:domain rdf:resource="http://visallo.org/insider-threat#employee"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


